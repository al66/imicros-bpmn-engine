{
    "name": "imicros-bpmn-engine",
    "version": "0.0.1",
    "description": "BPMN process engine",
    "main": "index.js",
    "directories": {
      "lib": "lib"
    },
    "scripts": {
      "test": "jest --coverage --no-cache",
      "test-engine": "jest test/engine.spec.js --no-cache"
    },
    "jest": {
      "collectCoverageFrom": [
        "lib/*.js",
        "lib/util/*.js"
      ],
      "coveragePathIgnorePatterns": [
        "/node_modules/",
        "/dev/"
      ],
      "testPathIgnorePatterns": [
        "/node_modules/",
        "/dev/"
      ],
      "rootDir": ".",
      "roots": [
        "./test"
      ],
      "moduleDirectories": [
        "node_modules"
      ]
    },
    "repository": {
      "type": "git",
      "url": "git+https://github.com/al66/imicros-bpmn-engine.git"
    },
    "keywords": [
      "imicros",
      "BPMN",
      "engine"
    ],
    "author": "Andreas Leinen <andreas.leinen@imicros.de> (www.imicros.de)",
    "license": "MIT",
    "bugs": {
      "url": "https://github.com/al66/imicros-bpmn-engine/issues"
    },
    "homepage": "https://github.com/al66/imicros-bpmn-engine#readme",
    "devDependencies": {
      "jest": "^29.7.0"
    },
    "dependencies": {
      "imicros-feel-interpreter": "^1.0.0",
      "fast-xml-parser": "^4.3.6",
      "uuid": "^9.0.1"
    }
  }
  